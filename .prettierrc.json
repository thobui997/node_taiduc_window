{
	"singleQuote": true,
	"arrowParens": "always",
	"printWidth": 100,
	"bracketSameLine": false,
	"bracketSpacing": true,
	"embeddedLanguageFormatting": "auto",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"jsxSingleQuote": false,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"semi": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"useTabs": false,
	"vueIndentScriptAndStyle": false
}
